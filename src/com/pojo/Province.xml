<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pojo.Province">
<select id="queryProvinceByID" parameterType="java.lang.String" resultType="com.pojo.Province">
select id,provinceID,province from hat_province where provinceID=#{num}


</select>


</mapper>